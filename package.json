{
  "name": "WebAutomationFrameworkCypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf -r ./cypress/reports/*.json",
    "cypress:open": "cypress open",
    "test": "npm run cypress-test || npm run posttest",
    "posttest": "npm run merge_report && npm run generate-htmlreport",
    "merge_report": "mochawesome-merge ./cypress/reports/*.json > ./test_report.json",
    "generate-htmlreport": "marge ./test_report.json --reportDir ./cypress/reports",
    "cypress-test": "cypress run --reporter mochawesome"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.2.0",
    "cypress": "^10.7.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "esbuild": "^0.15.7",
    "typescript": "^4.7.4"
  }
}
